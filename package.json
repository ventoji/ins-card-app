  "scripts": {
    "start": "node server/server.js",
    "build:dev": "webpack --mode=development --config ./config/webpack.dev.js",
    "build:prod": "webpack --mode=production --config ./config/webpack.dev.js",
    "dev-server": "webpack serve --mode=development --config ./config/webpack.dev.js",
    "test": "jest",
    "test:dev": "jest --watchAll",
    "test:coverage": "npm run test:dev -- --coverage --collectCoverageFrom",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "test:genreate-watch": "npm run test:generate-output -- --watch",
    "storybook": "start-storybook -p 6006",
    "prebuild:storybook": "npm run test:generate-output",
    "build:storybook": "build-storybook",
    "predeploy:stroybook": "npm run build-storybook",
    "deploy:storybook": "gh-pages -d storybook-static",
    "storybook-debug": "npm run storybook --debug-webpack",
    "lint": "eslint src/**/*.{js,jsx}",
    "lint:fix": "eslint src/**/*.{js,jsx} --fix",
    "prettier": "prettier --write src/**/*.{js,jsx,json,css,scss,md}",
    "ncu": "ncu",
    "ncu:updated": "ncu -u",
    "release": "standard-version"
  },
